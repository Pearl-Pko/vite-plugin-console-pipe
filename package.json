{
    "name": "vite-plugin-console-pipe",
    "description": "A package for development that forwards all logs from a client browser to the Vite dev server.",
    "version": "0.1.0",
    "packageManager": "pnpm@9.6.0",
    "type": "module",
    "license": "MIT",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Pearl-Pko/vite-plugin-console-pipe.git"
    },
    "author": "Pearl-Pko",
    "keywords": [
        "vite",
        "plugin",
        "console"
    ],
    "scripts": {
        "test": "vitest",
        "dev": "vite",
        "build": "tsup",
        "prepare": "npm run build"
    },
    "files": [
        "dist",
        "README.md"
    ],
    "peerDependencies": {
        "esbuild": "^0.25.0",
        "vite": "^6.2.0"
    },
    "devDependencies": {
        "@changesets/cli": "^2.28.1",
        "@eslint/js": "^9.21.0",
        "@testing-library/dom": "^10.4.0",
        "@types/node": "^22.13.5",
        "@vitest/browser": "^3.0.7",
        "esbuild": "^0.25.0",
        "eslint": "^9.21.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.3",
        "globals": "^16.0.0",
        "playwright": "^1.50.1",
        "prettier": "^3.5.2",
        "tsup": "^8.4.0",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.25.0",
        "vite": "^6.2.0",
        "vitest": "^3.0.7"
    }
